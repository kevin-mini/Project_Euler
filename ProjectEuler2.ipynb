    "#!/usr/bin/env/python\n",
    "#anuhya\n",
    "#Project Euler 2. Fibnocci series(up to 4 million)\n",
    "#22-05-2017\n",
    "\n",
    "a,b,c,s=0,1,0,0\n",
    "while(c<4000000):\n",
    "    c=a+b\n",
    "    a=b\n",
    "    b=c\n",
    "    if (c%2==0):\n",
    "        s=s+c\n",
    "    \n",
    "print \"the sum is {}\" .format(s)"
   ]
